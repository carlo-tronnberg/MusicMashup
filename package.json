{
  "name": "musicmashup",
  "version": "0.0.1",
  "description": "MusicBrainz-based Mashup API",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlo-tronnberg/MusicMashup.git"
  },
  "license": "Apache-2.0",
  "author": "Carlo Tr√∂nnberg",
  "main": "musicMashup.js",
  "scripts": {
    "build:postman": "portman --cliOptionsFile portman/portman-cli.json",
    "clean": "rimraf pact",
    "complexity": "cr src",
    "lint:fix": "eslint --cache --fix",
    "lint:verify": "eslint .",
    "pre-push-simple": "npm-run-all test:unit test:bdd",
    "pre-push": "npm-run-all test",
    "precommit": "lint-staged --relative --verbose",
    "prepare": "husky install",
    "prettier:fix": "prettier --write",
    "prettier:verify": "prettier --check \"*.{js,css,md,yml,json}\"",
    "save": "./scripts/save.sh",
    "start": "npm run start:app",
    "start:app": "node app.js",
    "start:console": "node musicMashup.js",
    "start:app:watch": "nodemon app.js",
    "start:swagger": "docker run --rm -p 3080:8080 -e SWAGGER_JSON=/definition/openapi.yml -v $(pwd)/test/mock/openapi.yaml:/definition/openapi.yaml swaggerapi/swagger-ui:latest",
    "test": "jest",
    "test:api": "npm run build:postman && newman run test/mock/postman_collection.json -r cli,json",
    "test:api:watch": "chokidar '*.js' 'src/*.js' 'test/mock/postman_collection.json' --initial -c 'npm run test:api'",
    "test:api:unit": "jest  --forceExit --testPathPattern='.*/test/api/.*.test.js'",
    "test:api:unit:watch": "npm run test:api:unit -- --watch",
    "test:bdd": "jest ./features",
    "test:bdd:watch": "npm run test:bdd -- --watch",
    "test:security": "snyk test",
    "test:security:dev": "npm run test:security -- --dev",
    "test:unit": "jest ./test --testPathIgnorePatterns test/api/*",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "create-badges": "jest-coverage-badges"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix"
    ],
    "*.{js,css,md,yml,json}": [
      "npm run prettier:fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "complexity-report": "^2.0.0-alpha",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^5.0.1",
    "jsonpath": "^1.1.1",
    "node-fetch": "^2.7.0",
    "supertest": "^6.2.2",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@apideck/portman": "^1.14.2",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/travis-cli": "^16.2.1",
    "@pact-foundation/absolute-version": "^0.0.4",
    "@pact-foundation/pact": "^9.17.3",
    "@pact-foundation/pact-node": "^10.17.2",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/newman": "^5.3.0",
    "@types/nodemon": "^1.19.1",
    "@types/prettier": "^2.4.4",
    "@types/rimraf": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.3",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-config": "^27.5.1",
    "jest-cucumber-fusion": "^0.8.1",
    "jest-pact": "^0.9.3",
    "lint-staged": "^12.5.0",
    "newman": "^5.3.2",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^16.3.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sonarqube-scanner": "^2.8.1",
    "sort-package-json": "^1.54.0",
    "supertest": "^6.3.1"
  },
  "engines": {
    "node": "v18.19.1"
  }
}
